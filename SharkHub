local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("SharkHub", "Ocean")

--Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")


MainSection:NewButton("Discord Server", "Check Output", function()
    print("https://discord.gg/AwcXAN7nzN")
end)

--Admins
local Admin = Window:NewTab("Admin")
local AdminSection = Admin:NewSection("Admin")


AdminSection:NewButton("Infinite Yield", "The offical IY LoadString", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)


AdminSection:NewButton("CMD-X", "Offical CMD-X LoadString", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source", true))()
end)


AdminSection:NewButton("Fates Admin", "Offical Fates Admin LoadString", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/fatesc/fates-admin/main/main.lua"))();end)

AdminSection:NewLabel("Admins For your Exploiting")


--LocalPlayer
local LocalPlayer = Window:NewTab("LocalPlayer")
local LocalPlayerSection = LocalPlayer:NewSection("LocalPlayer")


LocalPlayerSection:NewSlider("Walkspeed", "Changes Your Walkspeed", 500, 16, function(s) 
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)


LocalPlayerSection:NewSlider("JumpPower", "Changes Your JumpPower", 350, 50, function(s) 
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)


LocalPlayerSection:NewButton("Reset Settings", "Reset settings to default", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
	game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
end)


 
 --Scripts
local Scripts = Window:NewTab("Scripts")
local ScriptsSection = Scripts:NewSection("Scripts")

ScriptsSection:NewLabel("Hubs")

ScriptsSection:NewButton("Owl Hub", "Discountinued, But still a good Hub", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CriShoux/OwlHub/master/OwlHub.txt"))();
end)


ScriptsSection:NewButton("Dark Hub", "Decent UI,Supports many games", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/RandomAdamYT/DarkHub/master/Init", true))()
end)


ScriptsSection:NewButton("Hat Hub", "Oh dear, I dont need words to explain this..", function()
  loadstring(game:HttpGet("https://textbin.net/raw/vxrnz4q6p2"))();
end)


ScriptsSection:NewButton("Sniff hub", "This is a good Hub And with alot of features", function()
  loadstring(game:HttpGet('https://raw.githubusercontent.com/2dgeneralspam1/Sniff-Hub/main/Sniff%20Hub'))()
end)

ScriptsSection:NewLabel("Scripts")

ScriptsSection:NewButton("RemX", "Very good Stuff", function()
  loadstring(game:HttpGet(('https://raw.githubusercontent.com/Henry887/RemX-Script-Hub/main/main.lua'),true))()
end)


ScriptsSection:NewButton("FE Chat Translator", "info: https://controlc.com/6da2df96 ", function()
  loadstring(game:HttpGetAsync('https://i.qts.life/r/ChatInlineTranslator.lua', true))()
end)


ScriptsSection:NewButton("FE Talking Ben", "Who said a dog cant talk?", function()
  loadstring(game:HttpGet("https://raw.githubusercontent.com/BlastingStone/MyLuaStuff/master/benscriptgui.lua"))()
end)
